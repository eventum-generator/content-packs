{%- set record_id = shared.get('record_id', 1) -%}
{%- set hostname = params.hostname -%}
{%- set domain = params.domain -%}
{%- set fqdn = hostname ~ '.' ~ params.fqdn_suffix -%}
{%- set domain_sid = params.domain_sid -%}
{%- set subject_user = "Administrator" -%}
{%- set subject_sid = domain_sid ~ "-500" -%}
{%- set subject_logon_id = "0x" ~ module.rand.string.hex(5) -%}
{%- set target_user = (samples.usernames | random).username -%}
{%- set target_sid = domain_sid ~ "-" ~ module.rand.number.integer(1100, 9999) -%}
{%- set process_id = module.rand.number.integer(4, 65535) -%}
{%- set thread_id = module.rand.number.integer(1, 8191) -%}
{
    "@timestamp": "{{ timestamp.isoformat() }}",
    "agent": {
        "ephemeral_id": "{{ module.rand.crypto.uuid4() }}",
        "id": "{{ params.agent_id }}",
        "name": "{{ fqdn }}",
        "type": "winlogbeat",
        "version": "{{ params.agent_version }}"
    },
    "ecs": {
        "version": "8.17.0"
    },
    "event": {
        "action": "deleted-user-account",
        "category": ["iam"],
        "code": "4726",
        "kind": "event",
        "module": "security",
        "outcome": "success",
        "provider": "Microsoft-Windows-Security-Auditing",
        "type": ["user", "deletion"]
    },
    "host": {
        "name": "{{ hostname }}",
        "os": {
            "family": "windows",
            "type": "windows"
        }
    },
    "log": {
        "level": "information"
    },
    "related": {
        "user": ["{{ subject_user }}", "{{ target_user }}"]
    },
    "user": {
        "domain": "{{ domain }}",
        "id": "{{ subject_sid }}",
        "name": "{{ subject_user }}",
        "target": {
            "domain": "{{ domain }}",
            "id": "{{ target_sid }}",
            "name": "{{ target_user }}"
        }
    },
    "winlog": {
        "channel": "Security",
        "computer_name": "{{ fqdn }}",
        "event_data": {
            "PrivilegeList": "-",
            "SubjectDomainName": "{{ domain }}",
            "SubjectLogonId": "{{ subject_logon_id }}",
            "SubjectUserName": "{{ subject_user }}",
            "SubjectUserSid": "{{ subject_sid }}",
            "TargetDomainName": "{{ domain }}",
            "TargetSid": "{{ target_sid }}",
            "TargetUserName": "{{ target_user }}"
        },
        "event_id": "4726",
        "keywords": ["Audit Success"],
        "level": "information",
        "opcode": "Info",
        "outcome": "success",
        "process": {
            "pid": {{ process_id }},
            "thread": {
                "id": {{ thread_id }}
            }
        },
        "provider_guid": "{54849625-5478-4994-A5BA-3E3B0328C30D}",
        "provider_name": "Microsoft-Windows-Security-Auditing",
        "record_id": "{{ record_id }}",
        "task": "User Account Management",
        "time_created": "{{ timestamp.isoformat() }}"
    }
}
{%- do shared.set('record_id', (record_id + 1) % 2147483647) -%}
