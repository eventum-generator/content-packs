{%- set record_id = shared.get('record_id', 1) -%}
{%- set hostname = params.hostname -%}
{%- set domain = params.domain -%}
{%- set fqdn = hostname ~ '.' ~ params.fqdn_suffix -%}
{%- set domain_sid = params.domain_sid -%}
{#- Try to consume a process from the pool for correlation -#}
{%- set proc_pool = shared.get('processes', []) -%}
{%- if proc_pool | length > 0 -%}
  {%- set proc = proc_pool.pop(0) -%}
  {%- do shared.set('processes', proc_pool) -%}
  {%- set process_name = proc.name -%}
  {%- set process_path = proc.path -%}
  {%- set exit_pid = proc.pid -%}
  {%- set subject_user = proc.user -%}
  {%- set subject_domain = proc.domain -%}
  {%- set subject_sid = proc.sid -%}
  {%- set subject_logon_id = proc.logon_id -%}
{%- else -%}
  {%- set proc = samples.processes | random -%}
  {%- set process_name = proc.name -%}
  {%- set process_path = proc.path -%}
  {%- set exit_pid = "0x" ~ module.rand.string.hex(3) -%}
  {%- set subject_user = (samples.usernames | random).username -%}
  {%- set subject_domain = domain -%}
  {%- set subject_sid = domain_sid ~ "-" ~ module.rand.number.integer(1100, 9999) -%}
  {%- set subject_logon_id = "0x" ~ module.rand.string.hex(5) -%}
{%- endif -%}
{#- Exit status â€” mostly clean exits -#}
{%- set exit_status = module.rand.weighted_choice(["0x0", "0x1", "0xc0000005", "0xc000013a", "0x1"], [85, 5, 3, 5, 2]) -%}
{%- set winlog_pid = module.rand.number.integer(4, 65535) -%}
{%- set thread_id = module.rand.number.integer(1, 8191) -%}
{
    "@timestamp": "{{ timestamp.isoformat() }}",
    "agent": {
        "ephemeral_id": "{{ module.rand.crypto.uuid4() }}",
        "id": "{{ params.agent_id }}",
        "name": "{{ fqdn }}",
        "type": "winlogbeat",
        "version": "{{ params.agent_version }}"
    },
    "ecs": {
        "version": "{{ params.agent_version }}"
    },
    "event": {
        "action": "exited-process",
        "category": ["process"],
        "code": "4689",
        "kind": "event",
        "module": "security",
        "outcome": "success",
        "provider": "Microsoft-Windows-Security-Auditing",
        "type": ["end"]
    },
    "host": {
        "name": "{{ hostname }}",
        "os": {
            "family": "windows",
            "type": "windows"
        }
    },
    "log": {
        "level": "information"
    },
    "process": {
        "executable": "{{ process_path }}",
        "name": "{{ process_name }}",
        "pid": {{ exit_pid }}
    },
    "related": {
        "user": ["{{ subject_user }}"]
    },
    "user": {
        "domain": "{{ subject_domain }}",
        "id": "{{ subject_sid }}",
        "name": "{{ subject_user }}"
    },
    "winlog": {
        "channel": "Security",
        "computer_name": "{{ fqdn }}",
        "event_data": {
            "ProcessId": "{{ exit_pid }}",
            "ProcessName": "{{ process_path }}",
            "Status": "{{ exit_status }}",
            "SubjectDomainName": "{{ subject_domain }}",
            "SubjectLogonId": "{{ subject_logon_id }}",
            "SubjectUserName": "{{ subject_user }}",
            "SubjectUserSid": "{{ subject_sid }}"
        },
        "event_id": "4689",
        "keywords": ["Audit Success"],
        "level": "information",
        "opcode": "Info",
        "outcome": "success",
        "process": {
            "pid": {{ winlog_pid }},
            "thread": {
                "id": {{ thread_id }}
            }
        },
        "provider_guid": "{54849625-5478-4994-A5BA-3E3B0328C30D}",
        "provider_name": "Microsoft-Windows-Security-Auditing",
        "record_id": "{{ record_id }}",
        "task": "Process Termination",
        "time_created": "{{ timestamp.isoformat() }}"
    }
}
{%- do shared.set('record_id', record_id + 1) -%}
