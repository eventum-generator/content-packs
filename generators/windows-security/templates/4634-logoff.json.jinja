{%- set record_id = shared.get('record_id', 1) -%}
{%- set hostname = params.hostname -%}
{%- set domain = params.domain -%}
{%- set fqdn = hostname ~ '.' ~ params.fqdn_suffix -%}
{%- set domain_sid = params.domain_sid -%}
{#- Try to consume a session from the pool for correlation -#}
{%- set sessions = shared.get('sessions', []) -%}
{%- if sessions | length > 0 -%}
  {%- set session = sessions.pop(0) -%}
  {%- do shared.set('sessions', sessions) -%}
  {%- set target_user = session.user -%}
  {%- set target_sid = session.sid -%}
  {%- set logon_id = session.logon_id -%}
  {%- set logon_type = session.logon_type -%}
  {%- set logon_type_name = session.logon_type_name -%}
{%- else -%}
  {%- set target_user = (samples.usernames | random).username -%}
  {%- set target_sid = domain_sid ~ "-" ~ module.rand.number.integer(1100, 9999) -%}
  {%- set logon_id = "0x" ~ module.rand.string.hex(5) -%}
  {%- set logon_type = module.rand.weighted_choice(["3", "5", "10", "2"], [55, 20, 15, 10]) -%}
  {%- set logon_type_map = {"2": "Interactive", "3": "Network", "5": "Service", "10": "RemoteInteractive"} -%}
  {%- set logon_type_name = logon_type_map[logon_type] -%}
{%- endif -%}
{%- set process_id = module.rand.number.integer(4, 65535) -%}
{%- set thread_id = module.rand.number.integer(1, 8191) -%}
{
    "@timestamp": "{{ timestamp.isoformat() }}",
    "agent": {
        "ephemeral_id": "{{ module.rand.crypto.uuid4() }}",
        "id": "{{ params.agent_id }}",
        "name": "{{ fqdn }}",
        "type": "winlogbeat",
        "version": "{{ params.agent_version }}"
    },
    "ecs": {
        "version": "8.17.0"
    },
    "event": {
        "action": "logged-out",
        "category": ["authentication"],
        "code": "4634",
        "kind": "event",
        "module": "security",
        "outcome": "success",
        "provider": "Microsoft-Windows-Security-Auditing",
        "type": ["end"]
    },
    "host": {
        "name": "{{ hostname }}",
        "os": {
            "family": "windows",
            "type": "windows"
        }
    },
    "log": {
        "level": "information"
    },
    "related": {
        "user": ["{{ target_user }}"]
    },
    "user": {
        "domain": "{{ domain }}",
        "id": "{{ target_sid }}",
        "name": "{{ target_user }}"
    },
    "winlog": {
        "channel": "Security",
        "computer_name": "{{ fqdn }}",
        "event_data": {
            "LogonType": "{{ logon_type }}",
            "TargetDomainName": "{{ domain }}",
            "TargetLogonId": "{{ logon_id }}",
            "TargetUserName": "{{ target_user }}",
            "TargetUserSid": "{{ target_sid }}"
        },
        "event_id": "4634",
        "keywords": ["Audit Success"],
        "level": "information",
        "logon": {
            "id": "{{ logon_id }}",
            "type": "{{ logon_type_name }}"
        },
        "opcode": "Info",
        "outcome": "success",
        "process": {
            "pid": {{ process_id }},
            "thread": {
                "id": {{ thread_id }}
            }
        },
        "provider_guid": "{54849625-5478-4994-A5BA-3E3B0328C30D}",
        "provider_name": "Microsoft-Windows-Security-Auditing",
        "record_id": "{{ record_id }}",
        "task": "Logoff",
        "time_created": "{{ timestamp.isoformat() }}"
    }
}
{%- do shared.set('record_id', (record_id + 1) % 2147483647) -%}
